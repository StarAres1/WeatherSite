}

<h1>Загрузка файла Excel</h1>

<form asp-controller="WeatherData" asp-action="UploadFile" method="post" enctype="multipart/form-data">
    <input type="file" name="files" id="files" accept=".xlsx" required multiple />
    <button type="submit">Загрузить</button>
    <button type="button" onclick="document.getElementById('files').value = '';">Очистить выбор</button>
</form>